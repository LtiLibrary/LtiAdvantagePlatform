@page
@model LaunchPageModel

<style>
    body, html {
        margin: 0; padding: 0; height: 100%; overflow: hidden;
    }

    main.container-fluid {
        height: 100%;
        padding-top: 58px;
        padding-bottom: 0;
        padding-left: 0;
        padding-right: 0;
    }

    #iframe {
        width: 100%;
        height: 100%;
        border: none;
    }

    footer {
        display: none;
    }
</style>

<iframe id="iframe" src="@Url.Page("./LaunchFrame", new { id = Model.Request.Query["id"], personId = Model.Request.Query["personId"] })"></iframe>
